{
  "locals": {
    "url": "dev.pupil-labs.com",
    "name": "Pupil Labs",
    "owner": "Pupil Labs",
    "description": "Pupil Labs - accessible open source eye tracking platform"
  },
  "plugins": [
    "./plugins/paginator.coffee",
    "./plugins/featured-articles.coffee",
    "./plugins/blog.coffee",
    "./node_modules/wintersmith-contents",
    "./node_modules/wintersmith-sitemap"
  ],
  "shared": {
    "articles": "articles"
  },
  "require": {
    "moment": "moment",
    "_": "underscore",
    "typogr": "typogr"
  },
  "jade": {
    "pretty": true
  },
  "markdown": {
    "smartLists": true,
    "smartypants": true
  },
  "paginator": {
    "perPage": 6,
    "template": "blog.jade",
    "first": "blog/index.html",
    "filename": "blog/%d/index.html"
  },
  "ignore": [
    "**/.DS_Store"
  ]
}
