{# Single-Project template #}

{% extends "jumbotron.j2" %}


{% block container %}

  <div class="container">
  <div class="row">
    <div class="col-md-12 well well-lg">
      {% filter markdown -%}
          {% block overview %}{% endblock %}
      {%- endfilter %}
    </div><!-- /col -->
  </div><!-- /row -->

  <div class="row">
    <div class="col-md-12 well well-lg">
      {% block hardware %}{% endblock %}
    </div><!-- /col -->
  </div><!-- /row -->

  <div class="row">
    <div class="col-md-12 well well-lg well-table-pupil">
      <table class="table table-hover table-product-pupil">
          {% block price_table %}{% endblock %}
      </table>
    </div><!-- /col -->
  </div><!-- /.row -->

  <div class="row">
    <div class="col-md-12 well well-lg">
      {% filter markdown -%}
          {% block software %}{% endblock %}
      {%- endfilter %}
    </div><!-- /col -->
  </div><!-- /row -->

  <div class="row">
    <div class="col-md-12 well well-lg">
      {% filter markdown -%}
          {% block development %}{% endblock %}
      {%- endfilter %}
    </div><!-- /col -->
  </div><!-- /row -->

   <div class="row">
    <div class="col-md-6">
      {% filter markdown -%}
          {% block community %}{% endblock %}
      {%- endfilter %}
    </div><!-- /col -->
    <div class="col-md-6">
      {% filter markdown -%}
          {% block license %}{% endblock %}
      {%- endfilter %}
    </div><!-- /col -->
  </div><!-- /row -->

    <div class="row">

  </div><!-- /row -->

  </div><!-- /container -->

{% endblock container %}




